   "source": [
    "def strStr(haystack, needle):\n",
    "    import re\n",
    "    if needle in haystack:\n",
    "        match = re.search(needle,haystack)\n",
    "        x = match.span()[0]\n",
    "        return x\n",
    "    \n",
    "    else:\n",
    "        return -1\n",
    " "
   ]
